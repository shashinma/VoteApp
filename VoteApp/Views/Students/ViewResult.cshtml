@model IEnumerable<VoteApp.ViewModels.ResultViewModel>

@{
    ViewData["Title"] = "Student/ViewResult";
}

<h1>Exam Results</h1>

<div>
    <br />
    <div>
        <table id="resultGrid" class="table table-striped table-bordered dt-responsive nowarp"
               width="100" cellspacing="0">
            <thead>
            <tr>
                <th>Exam Name</th>
                <th>Total Questions/th>
                <th>Correct</th>
                <th>Wrong</th>
            </tr>
            </thead>
            <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.ExamName</td>
                    <td>@item.TotalQuestion</td>
                    <td>@item.CorrectAnswer</td>
                    <td>@item.WrongAnswer</td>
                </tr>
            }
            </tbody>
        </table>
    </div>
</div>

<div>
    <a asp-action="Profile">Profile</a>
</div>